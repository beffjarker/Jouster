{
  "enabledPlugins": [
    "file-explorer",
    "global-search",
    "switcher",
    "graph",
    "backlink",
    "tag-pane",
    "page-preview",
    "templates",
    "note-composer",
    "command-palette",
    "markdown-importer",
    "word-count",
    "outline"
  ],
  "alwaysUpdateLinks": true,
  "newFileLocation": "current",
