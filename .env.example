# .env.example - Template for environment variables
# Copy this file to .env and fill in your actual values

# Instagram Graph API Configuration (Current Standard)
INSTAGRAM_APP_ID=your_facebook_app_id_here
INSTAGRAM_APP_SECRET=your_facebook_app_secret_here
INSTAGRAM_ACCESS_TOKEN=your_instagram_graph_api_access_token_here
INSTAGRAM_USER_ID=your_instagram_business_user_id_here

# Facebook Page Configuration (Required for Instagram Graph API)
FACEBOOK_PAGE_ID=your_facebook_page_id_here
FACEBOOK_PAGE_ACCESS_TOKEN=your_page_access_token_here

# Backend Configuration
PORT=3000

# Development Environment
NODE_ENV=development

# Security Note: Never commit actual credentials to version control
# Use environment-specific configuration for production deployments
