{
  "name": "jouster-backend",
  "version": "1.0.0",
  "description": "Backend server for Jouster application with Instagram integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:security": "node tests/security.test.js",
    "security:audit": "npm audit",
    "security:audit:fix": "npm audit fix",
    "security:check": "node ../../security-checks.js",
    "security:full": "npm run security:audit && npm run security:check",
    "security:scan": "cd ../.. && cmd /c security-audit.bat",
    "security:scan:local": "node scripts/security-scan.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-vault": "^1.27.0",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "mailparser": "^3.7.5",
    "nodemailer": "^7.0.9",
    "uuid": "^10.0.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "express",
    "instagram",
    "api",
    "backend"
  ],
  "author": "",
  "license": "ISC"
}
