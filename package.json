    "@eslint/js": "^9.8.0",
    "@ngneat/eslint-plugin-reactive-forms": "^4.0.0",
    "@ngneat/spectator": "^21.0.1",
    "@nx/angular": "21.6.2",
    "@nx/cypress": "21.6.2",
    "@nx/eslint": "21.6.2",
    "@nx/eslint-plugin": "21.6.2",
    "@nx/jest": "21.6.2",
    "@nx/js": "21.6.2",
    "@nx/web": "21.6.2",
    "@nx/workspace": "21.6.2",
    "@schematics/angular": "20.3.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.12",
