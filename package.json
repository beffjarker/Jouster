{
  "name": "@jouster/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve jouster-ui --configuration=development",
    "serve": "nx serve jouster-ui --configuration=development",
    "serve:dev": "nx serve jouster-ui --host=localhost --port=4200 --configuration=development",
    "build": "nx build jouster-ui",
    "build:qa": "nx build jouster-ui --configuration=development",
    "build:prod": "nx build jouster-ui --configuration=production",
    "test": "nx test",
    "lint": "nx lint",
    "lint:fix": "nx lint --fix",
    "deploy:qa": "npm run build:qa && aws\\scripts\\deploy-qa.bat",
    "deploy:qa:sh": "npm run build:qa && ./aws/scripts/deploy-qa.sh",
    "deploy:staging": "npm run build:prod && aws\\scripts\\deploy-staging.bat",
    "deploy:staging:sh": "npm run build:prod && ./aws/scripts/deploy-staging.sh",
    "start:full": "node start-full-stack.js",
    "start:full:bat": "cmd /c start-full-stack.bat",
    "start:full:legacy": "npm run db:start && npm run db:wait && npm run db:init && npm run backend:start && npm run frontend:start",
    "start:full:health": "npm run db:start && npm run db:wait && npm run db:init && npm run backend:start:background && npm run backend:wait:60s && npm run frontend:start",
    "db:start": "docker-compose -f apps/backend/conversation-history/docker-compose.yml up -d",
    "db:wait": "powershell -ExecutionPolicy Bypass -File apps/backend/conversation-history/wait-for-database.ps1",
    "db:init": "cmd /c apps\\backend\\conversation-history\\init-tables.bat",
    "db:stop": "docker-compose -f apps/backend/conversation-history/docker-compose.yml down",
    "db:status": "docker ps --filter name=jouster",
    "backend:start": "cd apps/backend && npm run dev",
    "backend:start:background": "start \"Backend Server\" cmd /c \"cd apps/backend && npm run dev\"",
    "backend:wait:60s": "powershell -Command \"$timeout = 60; $elapsed = 0; do { try { $response = Invoke-WebRequest -Uri 'http://localhost:3001/health' -TimeoutSec 2 -UseBasicParsing; if ($response.StatusCode -eq 200) { Write-Host 'Backend is healthy!'; exit 0 } } catch { Start-Sleep -Seconds 2; $elapsed += 2 } } while ($elapsed -lt $timeout); Write-Host 'Backend health check timed out'; exit 1\"",
    "backend:debug": "cd apps/backend && node --inspect server.js",
    "frontend:start": "nx serve jouster-ui --configuration=development",
    "services:stop": "npm run db:stop",
    "cleanup:kill-node": "taskkill /F /IM node.exe || echo No Node processes to kill",
    "check:ports": "netstat -ano | findstr \":3001\\|:4200\\|:8000\\|:8001\"",
    "test:backend": "curl -X GET \"http://localhost:3001/health\" -H \"Content-Type: application/json\"",
    "test:emails": "curl -X GET \"http://localhost:3001/api/emails?pageSize=5\" -H \"Content-Type: application/json\"",
    "troubleshoot": "npm run check:ports && npm run test:backend && npm run test:emails",
    "test:e2e": "nx e2e jouster-ui-e2e",
    "test:e2e:headless": "nx e2e jouster-ui-e2e --headless",
    "test:e2e:watch": "nx e2e jouster-ui-e2e --watch"
  },
  "private": true,
